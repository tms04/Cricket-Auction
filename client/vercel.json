{
    "routes": [
        {
            "src": "/_next/static/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\.js)",
            "headers": {
                "content-type": "application/javascript"
            }
        },
        {
            "src": "/viewer",
            "dest": "/"
        },
        {
            "src": "/login",
            "dest": "/"
        },
        {
            "src": "/dashboard",
            "dest": "/"
        },
        {
            "src": "/tournament/(?<id>[^/]+)",
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": {
                "content-type": "text/html"
            }
        }
    ]
}